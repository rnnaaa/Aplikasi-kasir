	<queryString>
		<![CDATA[SELECT
    tk.id_transaksi_kasir,
    tk.tanggal,
    tk.total_harga,
    tk.diskon AS diskon_transaksi,
    tk.pembayaran,
    tk.kembalian,
    tk.metode_pembayaran,
    u.username AS kasir,

    dp.jumlah,
    dp.harga_satuan,
    dp.subtotal,
    dp.diskon AS diskon_item,
    bj.nama_barang

FROM transaksi_kasir tk
JOIN pengguna_aplikasi u ON tk.id_pengguna_aplikasi = u.id_pengguna
JOIN detail_transaksi_pelanggan dp ON dp.id_transaksi_kasir = tk.id_transaksi_kasir
JOIN barang_jual bj ON dp.id_barang_jual = bj.id_barang_jual

WHERE tk.id_transaksi_kasir = ?;]]>
	</queryString>
